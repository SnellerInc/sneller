SELECT
  0 + CASE x
    WHEN 1 THEN a
    WHEN 2 THEN b
    WHEN 3 THEN c
    ELSE d
  END AS y
FROM
  input
---
{"x": 1, "a": 11, "b": 111, "c": 1111, "d": 11111}
{"x": 2, "a": 22, "b": 222, "c": 2222, "d": 22222}
{"x": 3, "a": 33, "b": 333, "c": 3333, "d": 33333}
{"x": 7, "a": 77, "b": 777, "c": 7777, "d": 77777}
---
{"y": 11}
{"y": 222}
{"y": 3333}
{"y": 77777}
