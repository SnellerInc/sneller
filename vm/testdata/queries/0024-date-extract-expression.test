SELECT
  EXTRACT(YEAR    FROM DATE_ADD(SECOND, 1, t)) AS year,
  EXTRACT(QUARTER FROM DATE_ADD(SECOND, 1, t)) AS quarter,
  EXTRACT(MONTH   FROM DATE_ADD(SECOND, 1, t)) AS month,
  EXTRACT(DAY     FROM DATE_ADD(SECOND, 1, t)) AS day,
  EXTRACT(HOUR    FROM DATE_ADD(SECOND, 1, t)) AS hour,
  EXTRACT(MINUTE  FROM DATE_ADD(SECOND, 1, t)) AS minute,
  EXTRACT(SECOND  FROM DATE_ADD(SECOND, 1, t)) AS second
FROM input
