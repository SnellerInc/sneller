            "endpoint": "http://elastic:9200",
            "user": "elastic",
            "password": "ep-pass",
            "esPassword": "esp-pass",
            "ignoreCert": true
        },
        "sneller": {
            "endpoint": "http://snellerd:9180/",
            "token": "token",
            "timeout": 42
        },
        "memcache": {
            "endpoint": "127.0.0.1:1234",
