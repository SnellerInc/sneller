        },
        "mapping": {
            "flights": {
                "database": "test",
                "table": "flights",
                "ignoreTotalHits": true,
                "ignoreSumOtherDocCount": true
            },
            "news": {
                "database": "test",
                "table": "news",
                "ignoreTotalHits": true,
                "ignoreSumOtherDocCount": true
            }
        },
        "logFolder": "/var/log/proxy",
        "compareWithElastic": false
    }
