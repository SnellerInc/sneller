SELECT COUNT(x) + COUNT(DISTINCT x) + (1 + COUNT(DISTINCT x)) FROM input
---
WITH (
	ITERATE input FIELDS [x]
	FILTER DISTINCT [x]
	AGGREGATE COUNT(x) AS "count"
) AS REPLACEMENT(0)
ITERATE input FIELDS [x]
AGGREGATE COUNT(x) AS $_0_0
PROJECT $_0_0 + SCALAR_REPLACEMENT(0) + (SCALAR_REPLACEMENT(0) + 1) AS _1
